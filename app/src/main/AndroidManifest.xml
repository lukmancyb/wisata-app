<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.lomboktengahkab.wisataapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_wapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_wapp"
        android:supportsRtl="true"
        android:theme="@style/Theme.WisataApp"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup"
        tools:targetApi="m">
        <activity android:name=".ui.detail.DetailDestination" />
        <activity android:name=".ui.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.destination.DestinationListActivity" />
        <activity android:name=".ui.home.MainActivity"/>
    </application>

</manifest>